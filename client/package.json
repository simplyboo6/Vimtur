{
  "name": "client",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path dist",
    "build:watch": "ng build --watch --output-path dist",
    "build:prod": "ng build --configuration production --output-path dist",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check --watch --proxy-config ./proxy-config.json --port 8455",
    "watch": "ng build --watch --output-path dist",
    "lint": "ng lint && prettier --check '**/*.{ts,js,json,md,tsx,html,scss,css,yaml,yml}'",
    "fix": "ng lint --fix && prettier --check --write '**/*.{ts,js,json,md,tsx,html,scss,css,yaml,yml}'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@fortawesome/angular-fontawesome": "0.13",
    "@fortawesome/fontawesome-svg-core": "^6.5.0",
    "@fortawesome/free-solid-svg-icons": "^6.5.0",
    "@ng-bootstrap/ng-bootstrap": "15",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "5.2.3",
    "hls.js": "^1.5.11",
    "is-mobile": "^2.2.2",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.6.6",
    "socket.io-client": "^4.7.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular-eslint/builder": "16",
    "@angular-eslint/eslint-plugin": "16",
    "@angular-eslint/eslint-plugin-template": "16",
    "@angular-eslint/schematics": "16",
    "@angular-eslint/template-parser": "16",
    "@angular/cli": "^16.2.10",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^16.2.12",
    "@angular/localize": "16.2.12",
    "@types/node": "^14.0.22",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@vimtur/common": "../common",
    "eslint": "^8.2.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
