{
  "name": "client",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path dist",
    "build:watch": "ng build --watch --output-path dist",
    "build:prod": "ng build --configuration production --output-path dist",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check --watch --proxy-config ./proxy-config.json --port 8455",
    "watch": "ng build --watch --output-path dist",
    "lint": "ng lint && prettier --check '**/*.{ts,js,json,md,tsx,html,scss,css,yaml,yml}'",
    "fix": "ng lint --fix && prettier --check --write '**/*.{ts,js,json,md,tsx,html,scss,css,yaml,yml}'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/forms": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@angular/router": "^13.1.1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "bootstrap-css-only": "^4.4.1",
    "hls.js": "^1.1.2",
    "is-mobile": "^2.2.2",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.6.6",
    "socket.io-client": "^4.4.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.5",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^13.1.2",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "^13.1.1",
    "@angular/localize": "13.1.1",
    "@types/is-mobile": "^2.1.4",
    "@types/node": "^14.0.22",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@vimtur/common": "../common",
    "codelyzer": "^6.0.0",
    "eslint": "^8.2.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "~6.1.0",
    "typescript": "4.5.4"
  }
